    <script>pipeline {
    agent any

    tools {
        // Harus sesuai dengan nama yang kamu buat di Manage Jenkins &gt; Tools
        maven &apos;maven-3&apos;
    }

    stages {
        stage(&apos;Ambil Kode (Git)&apos;) {
            steps {
                // Jika kodinganmu ada di lokal folder yang sama dengan Jenkins
                // Jenkins otomatis sudah berada di workspace. 
                // Tapi jika pakai Git, masukkan: git &apos;https://github.com/user/repo.git&apos;
                echo &apos;Mengambil kode terbaru...&apos;
            }
        }

        stage(&apos;Build Java (Maven)&apos;) {
            steps {
                // Menjalankan build tanpa running test agar cepat saat demo
                sh &apos;mvn clean package -DskipTests&apos;
            }
        }

        stage(&apos;Update Container Docker&apos;) {
            steps {
                // Perintah ini akan mem-build ulang image dan me-restart container anggota
                // Pastikan nama servicenya sesuai dengan di docker-compose.yml
                sh &apos;docker-compose up -d --build anggotaservice&apos;
            }
        }
    }
    
    post {
        success {
            echo &apos;Selamat! Service Anggota berhasil diperbarui secara otomatis.&apos;
        }
        failure {
            echo &apos;Build gagal. Silakan cek Console Output untuk melihat errornya.&apos;
        }
    }
}</script>
